@model IEnumerable<OES.Model.Examination.Semester>

@{
    ViewBag.Title = "Index";
}
@section links
{
    @Html.Action("_AdminOptions", "Admins")
}
<h2>Semesters</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SemesterTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SemesterTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SemesterId }) |
            @Html.ActionLink("Details", "Details", new { id=item.SemesterId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SemesterId })
        </td>
    </tr>
}

</table>
